journal = {The Photogrammetric Record},
keywords = {3d building model,3d comparison,assessment,lidar,point cloud,quality,surface co-registration},
mendeley-groups = {Urban Reconstruction/Quality Measure,Urban Reconstruction/Quality Measure/Reference data},
number = {132},
pages = {339--355},
title = {{Quality assessment of 3D building data}},
url = {http://doi.wiley.com/10.1111/j.1477-9730.2010.00598.x},
volume = {25},
year = {2010}
}
